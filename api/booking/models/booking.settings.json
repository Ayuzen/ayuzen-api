{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "name": "Booking",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "checkin": {
      "required": true,
      "type": "date"
    },
    "checkout": {
      "type": "date",
      "required": true
    },
    "num_guests": {
      "type": "integer",
      "required": true
    },
    "travelling_with": {
      "type": "string"
    },
    "stay": {
      "model": "stay"
    },
    "purpose": {
      "type": "string"
    },
    "is_done": {
      "type": "boolean",
      "default": false
    }
  }
}
