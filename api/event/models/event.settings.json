{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text"
    },
    "Start": {
      "type": "datetime"
    },
    "End": {
      "type": "datetime"
    },
    "Cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "sessions": {
      "collection": "session"
    },
    "stays": {
      "collection": "stay"
    }
  }
}
